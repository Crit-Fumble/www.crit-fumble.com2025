{
  "name": "crit-fumble",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && npm run build:prisma:concepts && next build",
    "build:prisma:concepts": "cd packages/core-concepts-api && npx prisma generate",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate && npm run build:prisma:concepts",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run tests/unit/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:foundry": "playwright test tests/e2e/foundry-dual-system.spec.ts --project=chromium",
    "test:e2e:foundry:keep": "cross-env KEEP_DROPLET=true playwright test tests/e2e/foundry-dual-system.spec.ts --project=chromium",
    "test:cleanup": "bash scripts/cleanup-nul-files.sh",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:report": "playwright show-report test-results/html-report",
    "capture:ui": "tsx scripts/capture-ui.ts",
    "capture:ui:all-browsers": "tsx scripts/capture-ui.ts all-browsers",
    "capture:ui:mobile": "tsx scripts/capture-ui.ts mobile",
    "capture:ui:tablet": "tsx scripts/capture-ui.ts tablet",
    "capture:all": "playwright test tests/integration/10-login-dashboard-flow.spec.ts --grep=\"Visual Regression\" --project=chromium",
    "playwright:install": "playwright install --with-deps",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:dev": "bash scripts/db/migrate-env.sh dev",
    "db:migrate:staging": "bash scripts/db/migrate-env.sh staging",
    "db:migrate:prod": "bash scripts/db/migrate-env.sh production",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:setup:prod": "tsx scripts/db/setup-prod-database.ts",
    "db:cleanup-users": "tsx scripts/db/cleanup-users.ts",
    "scales:print": "tsx scripts/generate-print-scales.ts",
    "docker:build": "docker compose build",
    "mcp:build": "docker compose -f docker/docker-compose.test.yml build mcp-server",
    "mcp:up": "docker compose -f docker/docker-compose.test.yml up -d mcp-server",
    "mcp:down": "docker compose -f docker/docker-compose.test.yml stop mcp-server",
    "mcp:logs": "docker compose -f docker/docker-compose.test.yml logs -f mcp-server",
    "mcp:restart": "docker compose -f docker/docker-compose.test.yml restart mcp-server",
    "mcp:start": "cd mcp-server && npm start",
    "env:pull:staging": "vercel env pull .env.development.local --environment=preview",
    "env:pull:prod": "vercel env pull .env.production.local --environment=production",
    "test:staging": "cross-env PLAYWRIGHT_BASE_URL=https://treefarm22-staging.crit-fumble.com playwright test",
    "deploy:staging": "git push origin staging",
    "deploy:prod": "git push origin main",
    "dev:env:start": "bash scripts/dev/provision-dev-db.sh",
    "dev:env:stop": "bash scripts/dev/destroy-dev-db.sh",
    "dev:env:info": "bash scripts/dev/dev-db-info.sh",
    "prepare": "husky",
    "fumblebot": "cd src/packages/fumblebot && npm start",
    "fumblebot:build": "cd src/packages/fumblebot && npm run build",
    "fumblebot:dev": "cd src/packages/fumblebot && npm run dev",
    "fumblebot:install": "cd src/packages/fumblebot && npm install"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.70.1",
    "@auth/express": "^0.12.1",
    "@auth/prisma-adapter": "^2.7.2",
    "@discord/embedded-app-sdk": "^2.4.0",
    "@hookform/resolvers": "^3.9.1",
    "@playwright/test": "^1.57.0",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@stripe/stripe-js": "^4.8.0",
    "@tanstack/react-query": "^5.59.16",
    "@types/qrcode": "^1.5.6",
    "@vercel/blob": "^2.0.0",
    "axios": "^1.13.2",
    "canvas": "^3.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "discord-interactions": "^4.4.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "ffmpeg-static": "^5.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "jsonwebtoken": "^9.0.2",
    "limiter": "^3.0.0",
    "lucide-react": "^0.454.0",
    "next": "^16.0.3",
    "next-auth": "^5.0.0-beta.24",
    "next-themes": "^0.4.6",
    "openai": "^6.9.1",
    "pixi.js": "^8.14.1",
    "playwright": "^1.57.0",
    "puppeteer": "^24.30.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^8.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.53.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^17.2.1",
    "tailwind-merge": "^2.5.4",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.19.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.1.0",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.0.3",
    "husky": "^9.1.7",
    "postcss": "^8.4.47",
    "prisma": "^6.19.0",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^4.0.13"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=10.0.0"
  }
}
